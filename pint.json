{
    "preset": "laravel",
    "rules": {
        "concat_space": {
            "spacing": "one"
        },
        "not_operator_with_successor_space": false,
        "single_line_comment_style": false,
        "phpdoc_align": {
            "align": "left",
            "spacing": {
                "param": 1
            }
        }
    }
}